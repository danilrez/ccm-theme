{
	"name": "crappycode-theme",
	"displayName": "Flux Theme",
	"description": "Flux themes for VS Code",
	"version": "2.2.3",
	"author": "Danil Reznichenko [@danilrez]",
	"publisher": "CrappyCodeMaker",
	"license": "MIT",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#252B37",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danilrez/ccm-theme"
	},
	"bugs": {
		"url": "https://github.com/danilrez/ccm-theme/issues"
	},
	"engines": {
		"vscode": "^1.100.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"theme",
		"vscode-theme",
		"color-theme",
		"dark-theme",
		"light-theme",
		"ccm",
		"crappycodemaker",
		"danilrez",
		"reznida",
		"dark",
		"night",
		"light",
		"daylight"
	],
	"contributes": {
		"themes": [
			{
				"label": "Flux Night",
				"uiTheme": "vs-dark",
				"path": "themes/flux-night-theme.json"
			},
			{
				"label": "Flux Twilight",
				"uiTheme": "vs-dark",
				"path": "themes/flux-twilight-theme.json"
			},
			{
				"label": "Flux Daylight",
				"uiTheme": "vs",
				"path": "themes/flux-daylight-theme.json"
			}
		]
	},
	"files": [
		"CHANGELOG",
		"LICENSE",
		"README.md",
		"themes/**",
		"images/**"
	],
	"scripts": {
		"package": "vsce package -o ./${npm_package_name}-${npm_package_version}.vsix"
	}
}
